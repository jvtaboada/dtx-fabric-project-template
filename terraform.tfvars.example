#####################
# Arquivo de variáveis do projeto Terraform
# Copie este arquivo para terraform.tfvars e edite conforme seu ambiente
#####################

# ID do Tenant do Azure
tenant_id = "00000000-0000-0000-0000-000000000000"

# ID da Subscription do Azure
subscription_id = "00000000-0000-0000-0000-000000000000"

# Nome base do projeto no Fabric - utilizado para nomear recursos
base_name = "dtx-project"

# Ambiente de implantação: dev, hmg ou prd
environment = "dev"

# Região do Azure
location = "eastus"

# Nome de uma capacidade existente no Fabric
## Deixe vazio para criar uma nova capacidade
existing_fabric_capacity_name = ""

# SKU da capacidade do Fabric
sku = "F2"

# Lista de administradores da capacidade do Fabric
## Lista separada por vírgulas. Não adicione espaço depois da vírgula.
fabric_capacity_admins = "usuario1@empresa.com,usuario2@empresa.com"

# Nome do grupo de segurança (Entra) que contém os administradores do Workspace do Fabric
## Deixe vazio para adicionar os administradores manualmente
fabric_workspace_admins_sg_name=""